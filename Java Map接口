package com.company;
import java.util.*;
public class CollectionsDemo {
    public static void main(String[] args) {
        //使用Map只考虑一一对应就可
        Map m1 = new HashMap();//新建一个HashMap的接口
        m1.put("Zara", "8");//将指定的值与此映射中的指定键关联（可选操作）。
        m1.put("Mahnaz", "31");
        m1.put("Ayan", "12");
        m1.put("Daisy", "14");
        System.out.println();//输出空行
        System.out.println(" Map Elements");
        System.out.print("\t" + m1);//输出hashmap()中的下一个值
    }
}
